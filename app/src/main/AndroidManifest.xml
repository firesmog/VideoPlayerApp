<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.xiongying.videoplayerapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".account.AccountActivity">
        </activity>
        <activity android:name=".collection.CollectActivity">
        </activity>
        <activity android:name=".local.LocalVideoActivity">
        </activity>
        <activity android:name=".recommend.RecommendActivity">
        </activity>
        <activity android:name=".share.ShareActivity">
        </activity>
        <activity android:name=".upload.UploadActivity">
        </activity>
    </application>

</manifest>